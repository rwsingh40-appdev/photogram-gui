
<h1><%=@user_string%></h1>

<dl>
  <dt>ID</dt>
  <dd><%=@user.id%></dd>
  <dt>Edit user</dt>
  <dd>form placeholder</dd>
</dl>

<h1>Own photos(<%=@user.own_photos.length%>)</h1>

<table border="1">
  <tr>
    <th>Image</th>
    <th>Owner</th>
    <th>Caption</th>
    <th>Posted</th>
    <th></th>
  </tr>
  <%@user.own_photos.each do |a_photo|%>
    <tr>
      <td>
        <img src="<%=a_photo.image%>" alt="">
      </td>
      <td><%=@user_string%></td>
      <td><%=a_photo.caption%></td>
      <td><%=a_photo.updated_at%></td>
      <td>
        <a href="/photos/<%=a_photo.id%>">Show details</a>
      </td>
    </tr>
  <%end%>
</table>
